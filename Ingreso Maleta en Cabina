import java.util.Scanner;
  class Main {
  public static void main(String[] args) {
  // Definir las variables:
    Scanner teclado = new Scanner(System.in);
    Float A, B, C, D;

  // Ingreso de datos del usuario:
    System.out.println("Ingrese el peso de la maleta (kg): ");
    A = teclado.nextFloat();
    System.out.println("Ingrese el alto de la maleta (cm): ");
    B = teclado.nextFloat();
    System.out.println("Ingrese el largo de la maleta (cm): ");
    C = teclado.nextFloat();
    System.out.println("Ingrese el ancho de la maleta (cm): ");
    D = teclado.nextFloat();

  // Condiciones:
    if (A <= 10 && B <= 35 && C <= 55 && D <= 25)
    System.out.println("La maleta cumple con las condiciones puede llevarla en cabina.");
    else 
    System.out.println("La maleta no cumple con las condiciones no puede llevarla en cabina.");  
  }
}
